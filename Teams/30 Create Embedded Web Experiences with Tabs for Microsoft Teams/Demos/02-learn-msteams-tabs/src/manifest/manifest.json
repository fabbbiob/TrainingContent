  "staticTabs": [
    {
      "entityId": "default-data",
      "name": "LearnPersonalTab",
      "contentUrl": "https://{{HOSTNAME}}/learnPersonalTab/?theme={theme}",
      "scopes": [
        "personal"
      ]
    }
  ],
  "bots": [],
  "connectors": [],
  "composeExtensions": [],
